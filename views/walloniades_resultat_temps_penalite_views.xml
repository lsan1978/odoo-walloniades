<?xml version="1.0"?>
<odoo>
    <!-- Actions -->



    <!-- Walloniades Resultat Temps Pénalité Views -->

    <!--List View-->

    <record id="walloniades_resultat_temps_penalite_tree_view" model="ir.ui.view">
        <field name="name">walloniades.resultat.temps.penalite.list.view</field>
        <field name="model">walloniades.resultat.temps.penalite</field>
        <field name="arch" type="xml">
            <list editable="bottom" string="Res_temps_penalite">
                <field name="sequence" widget="handle"/>
                <field name="equipe_instance_id" readonly="1"/>
                <field name="temps_brut" widget="float" class="o_input_spinbox"/>
                <field name="penalites" widget="integer" class="o_input_spinbox"/>
            </list>
        </field>
    </record>

    <!--Form View-->

    <record id="walloniades_resultat_temps_penalite_form_view" model="ir.ui.view">
        <field name="name">walloniades.resultat.temps.penalite.form.view</field>
        <field name="model">walloniades.resultat.temps.penalite</field>
        <field name="arch" type="xml">
            <form string="Epreuve">
                <sheet>
                    <group>
                        <field name="equipe_instance_id"/>
                        <field name="temps_brut" widget="float" class="o_input_spinbox"/>
                        <field name="penalites" widget="integer" class="o_input_spinbox"/>
                        <field name="reso_set" invisible="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <!-- Search View -->



</odoo>